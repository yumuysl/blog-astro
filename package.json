    "@iconify-json/mdi": "^1.2.3",
    "@rollup/plugin-yaml": "^4.1.2",
    "@types/hast": "^3.0.4",
    "@types/markdown-it": "^14.1.2",
    "@types/mdast": "^4.0.4",
    "@types/qrcode": "^1.5.6",
    "@types/sanitize-html": "^2.16.0",
    "fontmin": "^1.1.1",
    "postcss-import": "^16.1.1",
    "postcss-nesting": "^13.0.2"
  }
}
